<?xml version="1.0"?>
<Defs>
  <RecipeDef>
    <defName>PE_ForgeIronArc</defName>
    <label>extract iron</label>
    <description>Exract pure iron from ore.</description>
    <jobString>extracting iron</jobString>
    <workAmount>500</workAmount>
    <recipeUsers>
      <li>PE_ArcFurnace</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>PE_RawIron</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>PE_RawIron</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <PE_Iron>1</PE_Iron>
    </products>
    <modExtensions>
      <li Class="ProductionExpanded.RecipeExtension_Processor">
        <ticksPerItemIn>200</ticksPerItemIn>
        <cycles>1</cycles>
        <capacityFactor>1</capacityFactor>
      </li>
    </modExtensions>
    <displayPriority>100</displayPriority>
  </RecipeDef>
