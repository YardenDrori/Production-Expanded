<?xml version="1.0"?>
<Defs>
  <RecipeDef>
    <defName>PE_ForgePlasteelCompositeArc</defName>
    <label>extract plasteel composite</label>
    <description>Extract plasteel composite from debris.</description>
    <jobString>refining plasteel</jobString>
    <workAmount>500</workAmount>
    <recipeUsers>
      <li>PE_ArcFurnace</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>PE_RawPlasteel</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>PE_RawPlasteel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <PE_UnrefinedPlasteel>1</PE_UnrefinedPlasteel>
    </products>
    <modExtensions>
      <li Class="ProductionExpanded.RecipeExtension_Processor">
        <isStaticRecipe>false</isStaticRecipe>
        <ingredients>
          <li>
            <thingDefs>
              <li>PE_RawPlasteel</li>
            </thingDefs>
            <count>1</count>
          </li>
        </ingredients>
        <products>
          <li>
            <output>PE_UnrefinedPlasteel</output>
            <count>1</count>
          </li>
        </products>
        <ratio>1.0</ratio>
        <ticksPerItemOut>2000</ticksPerItemOut>
        <cycles>1</cycles>
        <capacityFactor>5</capacityFactor>
      </li>
    </modExtensions>
    <displayPriority>300</displayPriority>
  </RecipeDef>
