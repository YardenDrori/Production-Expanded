<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>PE_ProcessDevilstrand</defName>
    <label>process devilstrand fungus into cloth</label>
    <description>Process raw devilstrand fungus logs into extremely durable and versatile cloth suitable for use in fabrics.</description>
    <jobString>Processing cloth.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Tailor</soundWorking>
    <workAmount>840</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawDevilstrand</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <DevilstrandCloth>1</DevilstrandCloth>
    </products>
    <skillRequirements>
      <Crafting>2</Crafting>
    </skillRequirements>
    <recipeUsers>
      <li>HandTailoringBench</li>
      <li>ElectricTailoringBench</li>
      <!-- <li>TableMachining</li> -->
    </recipeUsers>
    <displayPriority>950</displayPriority>
  </RecipeDef>
