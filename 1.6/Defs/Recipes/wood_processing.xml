  <RecipeDef>
    <defName>PE_ProcessWoodViaAutoSaw</defName>
    <label>refine lumber</label>
    <description>Refine logs into lumber.</description>
    <jobString>refining lumber</jobString>
    <workAmount>500</workAmount>
    <recipeUsers>
      <li>PE_AutoSawTable</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WoodRaw</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>WoodRaw</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <WoodLog>1</WoodLog>
    </products>
    <modExtensions>
      <li Class="ProductionExpanded.RecipeExtension_Processor">
        <ticksPerItemIn>1600</ticksPerItemIn>
        <cycles>1</cycles>
        <capacityFactor>1</capacityFactor>
      </li>
    </modExtensions>
  </RecipeDef>
