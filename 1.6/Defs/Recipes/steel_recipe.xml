<Defs>
  <RecipeDef>
    <defName>SmeltPigIron</defName>
    <label>smelt pig iron</label>
    <description>Smelt iron with carbon from logs to produce crude pig iron. Must be further refined in a blast furnace to create usable steel.</description>
    <jobString>Smelting pig iron.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>1920</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>PE_Iron</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <products>
      <PE_PigIron>8</PE_PigIron>
    </products>
    <skillRequirements>
      <Crafting>0</Crafting>
    </skillRequirements>
    <recipeUsers>
      <li>PE_FueledAlloySmelter</li>
      <li>EM_ElectricAlloySmelter</li>
    </recipeUsers>
    <displayPriority>15</displayPriority>
  </RecipeDef>

