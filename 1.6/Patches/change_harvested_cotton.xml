<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- Change all trees/plants that harvest WoodLog to harvest WoodRaw instead -->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef/plant[harvestedThingDef="Cloth"]/harvestedThingDef</xpath>
    <value>
      <harvestedThingDef>RawCotton</harvestedThingDef>
    </value>
  </Operation>
</Patch>
